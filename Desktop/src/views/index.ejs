<style>
     
  .notificationList-right {
         overflow: hidden;        
         white-space: nowrap;
         box-sizing: border-box;
         position: relative;
     }
     .notificationList-right ul {
         display: inline-block;
         padding: 0;
         margin: 0;
         list-style: none;
         animation: scroll-left 120s linear 0s infinite normal none running;
     }
     .notificationList-right li {
         display: inline;
         padding-right: 50px;
     }
     @keyframes scroll-left {
         0% {
             transform: translateX(100%);
         }
         100% {
             transform: translateX(-100%);
         }
     }
     .list-logo-game .listLogo-child ul li{
         position: relative;
         width: 60px;
         height: 60px;
         border-radius: 60px;
         display: flex;
         align-items: center;
         justify-content: center;
         margin: 5px 0 0 10px;
         background-color: #e7f1fb;
         cursor: pointer;
     }
     li{
         list-style-type: none;
     }
     .list-logo-game .listLogo-child ul li span{
         position: absolute;
         z-index: 1;
         opacity: 0;
         color: #399fda;
         transition: opacity 0.3s 0.2s, transform 0.3s 0.2s;
         font-size: 14px;
     }
     .list-logo-game .listLogo-child ul li:hover span{
         opacity: 1;
         transform: rotate(1turn);
     }
     .list-logo-game .listLogo-child ul li .logoBrandGame{
         transition: opacity 0.3s 0.2s, transform 0.3s 0.2s;
     }
     .list-logo-game .listLogo-child ul li:hover .logoBrandGame{
         opacity: 0;
         transform: rotate(180deg);
     }
     .nav--wapperListChild{
         display: inline-block;
         margin: 0 10px 10px 0;
         vertical-align: top;
         cursor: pointer;
     }
     .nav--wapperListChild span{
         display: inline-block;
         padding: 0 15px;
         color: #fff;
         border-radius: 15px;
         font-size: 15px;
         line-height: 30px;
     }
     .nav--wapperListChild:hover span{
         color: #fff;
         background: #642801;
     }
     .nav--wapperListChild:active span{
         color: #fff;
         background: #642801;
     }
     .Lobby-listChild{
      width: 289px;
      height: 312px;
      position: relative;
      overflow: hidden;
      margin: 5px 2px;
      cursor: pointer;
     }
     .Lobby-listChild img{
      width: 100%;
      
     }
     .containerListGame--child{
      height: 220px;
      padding: 10px 17px;
      display: flex;
      flex-direction: column;
      color: #535867;
      background-color: #e7f1fb;
      border-radius: 10px;
      text-align: center;
     }
     .containerListGame--child span{
      margin: 10px 0 5px;
      color: #535867;
      font-size: 14px;
      font-weight: bold;
     }
     .listGamePlayCompanyGame ul li{
      width: 180px;
      height: 250px;
      margin: 10px 20px 0 0;
     }
     
 </style>

